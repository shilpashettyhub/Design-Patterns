
public class Main {
	
	public static void main(String[] args) {
		
		System.out.println("Before creation of object\n Number of objects created : "+Singleton.getObjCreated());
		Singleton s1 = Singleton.getObject();
		System.out.println("After creation of first object\\n Number of objects created : "+Singleton.getObjCreated());
		Singleton s2 = Singleton.getObject();
		System.out.println("After creation of second object\\n Number of objects created : "+Singleton.getObjCreated());
		
		System.out.println(s1.getName());
		System.out.println(s2.getName());
		
	}

}
